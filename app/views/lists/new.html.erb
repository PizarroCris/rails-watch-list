<div class="row justify-content-center">
  <div class="col-4">
    <h1 class="mb-3">Create List:</h1>
    <%= simple_form_for @list do |f| %>
      <%= f.input :name %>
      <div class="d-grid">
        <%= f.submit "Create form", class: 'btn btn-success' %>
      </div>
    <% end %>
    <%= link_to "Back", root_path %>
  </div>
</div>


